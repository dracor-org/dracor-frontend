{
  "name": "dracor-frontend",
  "version": "0.36.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.32",
    "@fortawesome/free-brands-svg-icons": "5.15.1",
    "@fortawesome/free-solid-svg-icons": "5.15.1",
    "@fortawesome/react-fontawesome": "0.1.13",
    "CETEIcean": "^1.1.1",
    "apisauce": "^2.0.0",
    "axios": "^0.21.0",
    "bootstrap": "^4.5.3",
    "chart.js": "^2.9.4",
    "classnames": "^2.2.6",
    "http-proxy-middleware": "1.0.6",
    "prop-types": "^15.7.2",
    "react": "17.0.1",
    "react-bootstrap-table-next": "^3.1.3",
    "react-bootstrap-table2-toolkit": "^2.0.0",
    "react-chartjs-2": "^2.11.1",
    "react-dom": "17.0.1",
    "react-headroom": "^3.0.1",
    "react-helmet": "^5.2.1",
    "react-markdown": "^4.3.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "4.0.1",
    "react-sigma": "^1.2.35",
    "react-slick": "^0.26.1",
    "react-stickynode": "^3.0.4",
    "reactstrap": "8.0.0",
    "recharts": "^1.8.5",
    "swagger-ui-react": "^3.22.2",
    "yasgui": "^2.7.29"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deployment": "yarn run build && rsync -av build/ root@dracor.org:/var/www/html/"
  },
  "devDependencies": {
    "eslint": "^7.14.0",
    "eslint-config-xo-react": "0.23.0",
    "eslint-plugin-react": "7.21.5",
    "xo": "0.35.0"
  },
  "xo": {
    "extends": "xo-react",
    "space": true,
    "semicolon": true,
    "envs": [
      "browser"
    ],
    "rules": {
      "space-before-function-paren": [
        "warn",
        "always"
      ],
      "capitalized-comments": 0,
      "func-names": 0,
      "max-params": 0,
      "spaced-comment": 0,
      "import/no-unassigned-import": 0,
      "operator-linebreak": [
        1,
        "after",
        {
          "overrides": {
            "?": "before",
            ":": "before"
          }
        }
      ],
      "react/boolean-prop-naming": 1,
      "react/destructuring-assignment": [
        1,
        "always"
      ],
      "react/forbid-component-props": 0,
      "react/jsx-one-expression-per-line": 0,
      "react/jsx-no-bind": 0,
      "react/jsx-space-before-closing": 0,
      "react/jsx-tag-spacing": 1,
      "react/prop-types": [
        1,
        {
          "ignore": []
        }
      ],
      "react/static-property-placement": 0,
      "unicorn/filename-case": 0,
      "unicorn/no-reduce": 1,
      "unicorn/prefer-includes": 1,
      "unicorn/prevent-abbreviations": 0
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
